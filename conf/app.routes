# microservice specific routes

POST        /faststream/callback             @uk.gov.hmrc.fsetlaunchpadgateway.controllers.CallbackController.present()

POST        /faststream/register        @uk.gov.hmrc.fsetlaunchpadgateway.controllers.ApplicationController.createCandidate()
POST        /faststream/invite          @uk.gov.hmrc.fsetlaunchpadgateway.controllers.ApplicationController.inviteCandidate()
POST        /faststream/reset           @uk.gov.hmrc.fsetlaunchpadgateway.controllers.ApplicationController.resetCandidate()
POST        /faststream/retake          @uk.gov.hmrc.fsetlaunchpadgateway.controllers.ApplicationController.retakeCandidate()
POST        /faststream/extend          @uk.gov.hmrc.fsetlaunchpadgateway.controllers.ApplicationController.extendCandidate()
